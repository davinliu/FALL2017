    "function f(x)\n",
    "    return -x^2\n",
    "end\n",
    "a=0\n",
    "b=1\n",
    "tol=1.e-8\n",
    "d=1.e5\n",
    "while abs(d)>tol\n",
    " x1=0+((3-sqrt(5))/2)*(b-a)\n",
    " x2=0+((sqrt(5)-1)/2)*(b-a)\n",
    "    if f(x1)>f(x2)\n",
    "        b=x2\n",
    "    else\n",
    "        a=x1\n",
    "    end\n",
    "    d=f(a)-f(b)\n",
    "end\n",
    "println(a)"
   ]
